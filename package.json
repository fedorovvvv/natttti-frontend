{
	"name": "natti-site",
	"version": "1.0.1",
	"private": true,
	"author": "Nikita Fedorov <nikitafedorov7@yandex.ru>",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run smui-theme && vite build",
		"preview": "vite preview",
		"start": "node index.js",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check ./src && eslint ./src",
		"format": "prettier --plugin-search-dir . --write ./src",
		"prepare": "npm run smui-theme",
		"pocketbase:typegen": "pocketbase-typegen --env -o ./src/lib/shared/api/pocketbase/types.ts",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"smui-theme": "npm run smui-theme-light && npm run smui-theme-dark"
	},
	"devDependencies": {
		"@feature-sliced/eslint-config": "git+https://github.com/cue-cue/svelte-fsd-eslint-config.git",
		"@melt-ui/pp": "^0.1.4",
		"@melt-ui/svelte": "^0.61.2",
		"@smui/button": "^7.0.0-beta.8",
		"@smui/common": "^7.0.0-beta.8",
		"@smui/icon-button": "^7.0.0-beta.8",
		"@smui/ripple": "^7.0.0-beta.8",
		"@smui/select": "^7.0.0-beta.8",
		"@smui/textfield": "^7.0.0-beta.8",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.2",
		"@sveltejs/kit": "^2.4.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/js-cookie": "^3.0.3",
		"@types/node": "^20.2.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-boundaries": "^3.1.1",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte": "^2.30.0",
		"pocketbase-typegen": "^1.2.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@cluue/icons": "^1.0.24",
		"@cluue/styles": "^1.0.12",
		"@material/shape": "^14.0.0",
		"@octokit/types": "^9.2.3",
		"@tanstack/svelte-query": "^5.8.4",
		"@types/lodash": "^4.14.194",
		"autoprefixer": "^10.4.14",
		"chart.js": "^4.3.0",
		"chartjs-adapter-dayjs-4": "^1.0.4",
		"cookie": "^0.5.0",
		"date-picker-svelte": "^2.4.3",
		"dayjs": "^1.11.7",
		"devtools-detect": "^4.0.1",
		"js-cookie": "^3.0.5",
		"lodash": "^4.17.21",
		"octokit": "^2.0.19",
		"pocketbase": "^0.19.0",
		"sass": "^1.62.1",
		"svelte-chartjs": "^3.1.2",
		"svelte-material-ui": "^7.0.0-beta.8",
		"svelte-preprocess": "^5.0.3",
		"vite-plugin-svg-sprite": "^0.5.1",
		"yup": "^1.3.2"
	}
}
